<?xml version="1.0"?>
<ruleset name="JHU">
  <description>JHU coding standard</description>
  <rule ref="PSR2">
    <exclude name="Generic.Files.LineLength"/>
  </rule>

  <rule ref="Generic.WhiteSpace.ScopeIndent">
    <properties>
      <property name="indent" value="2"/>
    </properties>
  </rule>

</ruleset>
